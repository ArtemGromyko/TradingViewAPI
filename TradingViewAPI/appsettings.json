{
  "IEXCloudUrls": {
    "version": "stable",
    "baseUrl": "https://sandbox.iexapis.com",
    "historicalPricesUrl": "/stock/{0}/chart/max",
    "symbolsUrl": "/ref-data/symbols",
    "dividendsUrl": "/time-series/advanced_dividends",
    "exchangesUrl": "/ref-data/exchanges",
    "quotesUrl": "/stock/{0}/quote",
    "intradayPricesUrl": "/stock/{0}/intraday-prices"
  },
  "HttpClientName": "IexCloudClient",
  "MongoDBCollectionNames": {
    "SymbolsCollectionName": "SymbolsCollection",
    "ExchangesCollectionName": "ExchangesCollection",
    "DividendsCollectionName": "DividendsCollection",
    "HistoricalPricesCollectionName": "HistoricalPricesCollection",
    "CEOCompensationCollectionName": "CEOCompensationCollection",
    "QuotesCollectionName": "QuotesCollection",
    "IntradayPricesCollectionName": "IntradayPricesCollection"
  },
  "DatabaseSettings": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "TradingViewStore"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
